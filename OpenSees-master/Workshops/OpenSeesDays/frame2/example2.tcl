# source in the modelsource example1.tcl#set loads constant & reset time in domain to be 0.0loadConst -time 0.0# add the lateral load patternpattern Plain 2 Linear {    load 3 200.0 0.0 0.0    load 4 200.0 0.0 0.0}# do some cyclic loops using load control schemeforeach {numSteps stepSize} {10 0.1 10 -0.1 10 -0.1 10 0.1 10 0.1} {    integrator LoadControl $stepSize    analyze $numSteps        set time [getTime]    set disp [nodeDisp 3 1]    puts "Time: $time \tDisplacement: $disp" }print node 3