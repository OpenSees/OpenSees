# Create the reliability model builder ------------------------------------------------------------
reliability
model basic -ndm 2

# CREATE RANDOM VARIABLES
randomVariable	1	lognormal	-mean 100.0		-stdv 20.0
randomVariable	2	lognormal	-mean 15.0		-stdv 3.0

randomVariable	3	type2LargestValue		-mean 7.0		-stdv 2.1
randomVariable	4	type2LargestValue		-mean 5.0		-stdv 1.5

# SPECIFY CORRELATION
correlate   1 2  0.2
correlate	3 4  0.3

# PARAMETERS
parameter 1 randomVariable 1
parameter 2 randomVariable 2
parameter 3 randomVariable 3
parameter 4 randomVariable 4

# DEFINE LIMIT-STATE FUNCTION(s)
performanceFunction 1 "\$par(1) - 4.0*\$par(3) - 4.0*\$par(4)"
performanceFunction 2 "\$par(1) - 2.0*\$par(3) - 7.0*\$par(4)"
performanceFunction 3 "\$par(2) - 0.2*\$par(3) - 0.7*\$par(4)"
performanceFunction 4 "2.5*\$par(1) - 10.0*\$par(3) - 5.0*\$par(4)"
performanceFunction 5 "\$par(1) - 10.0*\$par(4)"
performanceFunction 6 "\$par(2) - 0.05*\$par(1) - 0.5*\$par(4)"
performanceFunction 7 "2.0*\$par(1) - 5.0*\$par(3) - 10.0*\$par(4)"
performanceFunction 8 "-\$par(1) + 10.0*\$par(2)"
performanceFunction 9 "\$par(1) - 10.0*\$par(3) - 20.0*\$par(4)"

# define cutsets
cutset 1	1 4
cutset 2	1 5
cutset 3	1 6
cutset 4	2 7
cutset 5	2 -5
cutset 6	2 8
cutset 7	3 9

# CREATE NECESSARY RELIABILITY ANALYSIS TOOLS
probabilityTransformation    Nataf            -print 1
reliabilityConvergenceCheck  Standard         -e1 1.0e-3    -e2 1.0e-3  -print 1
functionEvaluator            Tcl
gradientEvaluator            FiniteDifference -pert 1000
searchDirection              iHLRF
meritFunctionCheck           AdkZhang         -multi 2.0    -add 10.0   -factor 0.5  
#stepSizeRule                 Armijo           -maxNum 5    -base 0.5   -initial 1.0 2  -print 0
stepSizeRule                 Fixed            -stepSize 1.0
startPoint                   Mean
findDesignPoint              StepSearch       -maxNumIter 30

# Run the FORM analysis
runFORMAnalysis		CalRel_manual_8_output/8_FORM.out   -relSens 1

# Run the SORM analysis
#findCurvatures				 bySearchAlgorithm 5
findCurvatures		firstPrincipal
runSORMAnalysis		CalRel_manual_8_output/8_SORM_pc.out

hessianEvaluator			FiniteDifference -pert 1000
probabilityTransformation   Nataf           -print 0
findCurvatures				curvatureFitting
runSORMAnalysis				CalRel_manual_8_output/8_SORM_cf.out

# Now reset probability transformation so we don't get as much output for simulation
probabilityTransformation    Nataf           -print 0
randomNumberGenerator        CStdLib
runImportanceSamplingAnalysis CalRel_manual_8_output/8_SAMPLEa.out -type responseStatistics -maxNum 50000 -targetCOV 0.01 -print 0
runImportanceSamplingAnalysis CalRel_manual_8_output/8_SAMPLEb.out -type failureProbability -maxNum 50000 -targetCOV 0.01 -print 0

# Now run some system analyses using simplified and generalized systems
runSystemAnalysis	CalRel_manual_8_output/8_SERIES_pcm.out   PCM   allInSeries
runSystemAnalysis	CalRel_manual_8_output/8_SERIES_ipcm.out  IPCM  allInSeries
runSystemAnalysis	CalRel_manual_8_output/8_SERIES_mvn.out   MVN   allInSeries  -Nmax 50000 -tol 1.0e-8
runSystemAnalysis	CalRel_manual_8_output/8_SERIES_scis.out  SCIS  allInSeries  -Nmax 50000 -tol 1.0e-8

runSystemAnalysis	CalRel_manual_8_output/8_SYSTEM_pcm.out   PCM   cutsets
runSystemAnalysis	CalRel_manual_8_output/8_SYSTEM_ipcm.out  IPCM  cutsets
runSystemAnalysis	CalRel_manual_8_output/8_SYSTEM_mvn.out   MVN   cutsets  -Nmax 50000 -tol 1.0e-8
runSystemAnalysis	CalRel_manual_8_output/8_SYSTEM_scis.out  SCIS  cutsets  -Nmax 50000 -tol 1.0e-8

printReliability 1